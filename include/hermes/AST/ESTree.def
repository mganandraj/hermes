ESTREE_NODE_5_ARGS(
    FunctionDeclaration, FunctionLike,
    NodePtr, id, true,
    NodeList, params, false,
    NodePtr, body, false,
    NodePtr, returnType, true,
    NodeBoolean, generator, false)
ESTREE_LAST(FunctionLike)

